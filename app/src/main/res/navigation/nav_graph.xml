<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/handlerFragment">

    <fragment
        android:id="@+id/handlerFragment"
        android:name="dam.a42363.trailblaze.HandlerFragment"
        android:label="fragment_handler"
        tools:layout="@layout/fragment_handler">

        <action
            android:id="@+id/action_handlerFragment_to_explorarFragment"
            app:destination="@id/explorarFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />

    </fragment>
    <fragment
        android:id="@+id/explorarFragment"
        android:name="dam.a42363.trailblaze.ExplorarFragment"
        android:label="fragment_explorar"
        tools:layout="@layout/fragment_explorar">

        <action
            android:id="@+id/action_explorarFragment_to_signInFragment"
            app:destination="@id/signInFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />

        <action
            android:id="@+id/action_explorarFragment_to_perfilFragment"
            app:destination="@id/perfilFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_explorarFragment_to_fullInfoFragment"
            app:destination="@id/fullInfoFragment" />
        <action
            android:id="@+id/action_explorarFragment_to_explorarListFragment"
            app:destination="@id/explorarListFragment" />
        <action
            android:id="@+id/action_explorarFragment_to_navigationFragment"
            app:destination="@id/navigationFragment" />

    </fragment>
    <fragment
        android:id="@+id/guardadosFragment"
        android:name="dam.a42363.trailblaze.GuardadosFragment"
        android:label="fragment_guardados"
        tools:layout="@layout/fragment_guardados">

        <action
            android:id="@+id/action_guardadosFragment_to_signInFragment"
            app:destination="@id/signInFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/contribuirFragment"
        android:name="dam.a42363.trailblaze.ContribuirFragment"
        android:label="fragment_contribuir"
        tools:layout="@layout/fragment_contribuir">

        <action
            android:id="@+id/action_contribuirFragment_to_signInFragment"
            app:destination="@id/signInFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_contribuirFragment_to_partilharFragment"
            app:destination="@id/partilharFragment" />

    </fragment>
    <fragment
        android:id="@+id/perfilFragment"
        android:name="dam.a42363.trailblaze.PerfilFragment"
        android:label="fragment_perfil"
        tools:layout="@layout/fragment_perfil">

        <action
            android:id="@+id/action_perfilFragment_to_signInFragment"
            app:destination="@id/signInFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_perfilFragment_to_amigosFragment"
            app:destination="@id/amigosFragment" />

    </fragment>
    <fragment
        android:id="@+id/signInFragment"
        android:name="dam.a42363.trailblaze.SignInFragment"
        android:label="fragment_sign_in"
        tools:layout="@layout/fragment_sign_in">

        <action
            android:id="@+id/action_signInFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />

        <action
            android:id="@+id/action_signInFragment_to_registarFragment"
            app:destination="@id/registarFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />

        <action
            android:id="@+id/action_signInFragment_to_perfilFragment"
            app:destination="@id/perfilFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />

    </fragment>
    <fragment
        android:id="@+id/registarFragment"
        android:name="dam.a42363.trailblaze.RegistarFragment"
        android:label="fragment_registar"
        tools:layout="@layout/fragment_registar">

        <action
            android:id="@+id/action_registarFragment_to_criarPerfilFragment"
            app:destination="@id/criarPerfilFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />

    </fragment>
    <fragment
        android:id="@+id/criarPerfilFragment"
        android:name="dam.a42363.trailblaze.CriarPerfilFragment"
        android:label="fragment_criar_perfil"
        tools:layout="@layout/fragment_criar_perfil">

        <action
            android:id="@+id/action_criarPerfilFragment_to_perfilFragment"
            app:destination="@id/perfilFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />

    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="dam.a42363.trailblaze.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login">

        <action
            android:id="@+id/action_loginFragment_to_perfilFragment"
            app:destination="@id/perfilFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />

    </fragment>
    <fragment
        android:id="@+id/fullInfoFragment"
        android:name="dam.a42363.trailblaze.FullInfoFragment"
        android:label="fragment_full_info"
        tools:layout="@layout/fragment_full_info" >
        <action
            android:id="@+id/action_fullInfoFragment_to_navigationFragment"
            app:destination="@id/navigationFragment" />
    </fragment>
    <fragment
        android:id="@+id/explorarListFragment"
        android:name="dam.a42363.trailblaze.ExplorarListFragment"
        android:label="fragment_explorar_list"
        tools:layout="@layout/fragment_explorar_list" >
        <action
            android:id="@+id/action_explorarListFragment_to_explorarFragment"
            app:destination="@id/explorarFragment" />
        <action
            android:id="@+id/action_explorarListFragment_to_fullInfoFragment"
            app:destination="@id/fullInfoFragment" />
    </fragment>
    <fragment
        android:id="@+id/navigationFragment"
        android:name="dam.a42363.trailblaze.NavigationFragment"
        android:label="fragment_navigation"
        tools:layout="@layout/fragment_navigation" >
        <action
            android:id="@+id/action_navigationFragment_to_cameraFragment"
            app:destination="@id/cameraFragment" />
    </fragment>
    <fragment
        android:id="@+id/partilharFragment"
        android:name="dam.a42363.trailblaze.PartilharFragment"
        android:label="fragment_partilhar"
        tools:layout="@layout/fragment_partilhar" >
        <action
            android:id="@+id/action_partilharFragment_to_explorarFragment"
            app:destination="@id/explorarFragment" />
    </fragment>
    <fragment
        android:id="@+id/amigosFragment"
        android:name="dam.a42363.trailblaze.AmigosFragment"
        android:label="fragment_amigos"
        tools:layout="@layout/fragment_amigos" >
        <action
            android:id="@+id/action_amigosFragment_to_addAmigosFragment"
            app:destination="@id/addAmigosFragment" />
    </fragment>
    <fragment
        android:id="@+id/addAmigosFragment"
        android:name="dam.a42363.trailblaze.AddAmigosFragment"
        android:label="fragment_add_amigos"
        tools:layout="@layout/fragment_add_amigos" />
    <fragment
        android:id="@+id/cameraFragment"
        android:name="dam.a42363.trailblaze.CameraFragment"
        android:label="fragment_camera"
        tools:layout="@layout/fragment_camera" >
        <action
            android:id="@+id/action_cameraFragment_to_navigationFragment"
            app:destination="@id/navigationFragment" />
    </fragment>
</navigation>